{
  "name": "sowmya-art-gallery",
  "version": "0.0.1",
  "description": "Sowmya's Art Gallery",
  "private": true,
  "main": "js/bundle.js",
  "scripts": {
    "start": "watchify -v --debug -e js/index.js -o js/bundle.js",
    "build": "browserify --debug -e js/index.js -o js/bundle.js",
    "beefy": "beefy js/index.js:js/bundle.js --live --open",
    "serve": "live-server --watch=js/bundle.js,index.html,styles/app.css",
    "less": "lessc styles/app.less > styles/app.css",
    "browprod": "NODE_ENV=production browserify -e js/index.js -o js/bundle.js -g uglifyify",
    "prod": "webpack --progress -p",
    "lint": "eslint js/components",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Dheeraj V S",
  "license": "Proprietary",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "lodash": "^4.14.1",
    "material-ui": "^0.15.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-ga": "^2.1.1",
    "react-router": "^2.6.1",
    "react-router-scroll": "^0.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-plugin-react": "^6.0.0",
    "style-loader": "^0.13.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0",
    "webpack": "^1.13.0"
  }
}
